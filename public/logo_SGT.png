<script>
    $(document).ready(function () {
        function delay(callback, ms) {
            var timer = 0;
            return function () {
                var context = this, args = arguments;
                clearTimeout(timer);
                timer = setTimeout(function () {
                    callback.apply(context, args);
                }, ms || 0);
            };
        }

        var data_json = [];
        $("input#check_api").keydown(function () {
            var check = $(this).val();
            var arr = new Array();
            data_json.forEach(function (element) {
                var str1 = element.value;
                if (str1.indexOf(check) != -1 || str1.indexOf(check.toUpperCase()) != -1) {
                    arr.push(element);
                }
            })

            $(this).autocomplete({
                source: arr,
                select: function (event, ui) {
                    window.open(ui.item.url, '_self');
                    ;
                }
            });
        });


    });
	</script>

